# Integration.app workspace credentials
# Get these from your Integration.app settings page
INTEGRATION_APP_WORKSPACE_KEY=your_workspace_key_here
INTEGRATION_APP_WORKSPACE_SECRET=your_workspace_secret_here

# MongoDB connection string (required)
# Example: mongodb://username:password@host:port/database
MONGODB_URI=


# AWS credentials (required for S3)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-2
AWS_BUCKET_NAME=your_bucket_name_here
